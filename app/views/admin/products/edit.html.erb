<h1>edit product</h1>
<%= form_with(model: @product, url: admin_product_path(@product),method: :patch,  data: {turbo: false}) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.label :price %>
    <%= f.text_field :price %>

    <%= f.submit "edit Product" %>
    <%= link_to "back", admin_products_path %>
<% end %>
